{
    "name": "warmar/laravel-ai-translate",
    "description": "Complete multilingual framework for Laravel with AI translation, automatic routing, SEO optimization, and RTL support",
    "type": "library",
    "keywords": [
        "laravel",
        "multilingual",
        "translation",
        "i18n",
        "localization",
        "openai",
        "ai-translation",
        "seo",
        "routing",
        "rtl",
        "hreflang"
    ],
    "homepage": "https://warmardev.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Warmar",
            "email": "contact@warmardev.com",
            "homepage": "https://warmardev.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/warmar94/laravel-ai-translate/issues",
        "source": "https://github.com/warmar94/laravel-ai-translate",
        "docs": "https://warmardev.com/docs/laravel-translate.html"
    },
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": ">=11.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Warmar\\LaravelAiTranslate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Warmar\\LaravelAiTranslate\\LaravelAiTranslateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}